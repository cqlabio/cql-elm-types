{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "isolatedModules": true,
  },
  "include": [
    "**/*.ts",
    "index.d.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
